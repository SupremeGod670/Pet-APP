<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.PetAPP" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
         <item name="colorPrimary">@color/my_dark_primary</item>
    </style>

    <style name="BACKGROUND">
        <item name="android:background">@color/my_dark_primary</item>
    </style>

    <style name="TEXTCOLOR">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="HINTCOLOR">
        <item name="android:textColorHint">@color/white</item>
    </style>

    <style name="BUTTON">
        <item name="android:backgroundTint">#00BCD4</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="LINK">
        <item name="android:textColor">#00BCD4</item>
    </style>

    <style name="SEARCHCOLOR">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:color">@color/black</item>
    </style>

    <style name="TEXTLINEAR">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="PERFIL">
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="BUTTONSAIR">
        <item name="android:backgroundTint">@color/red</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ThemeNihgt.PetAPP" parent="Base.Theme.PetAPP" />
</resources>